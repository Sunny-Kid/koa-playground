{
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "plugins": ["prettier", "import"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:import/recommended"],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".d.ts", ".ts"]
      }
    }
  },
  "rules": {
    "max-len": [
      "error",
      {
        "code": 120
      }
    ]
  }
}
